extends layout
block title
  |  Status List
block content
  div.col-md-12
    h3 Status
    if(messages.success)
      p.bg-success
        = messages.success
    script.
      window.__INITIAL_STATE__ = { statuslist: !{JSON.stringify(sessions)} };
    div#mount
    if(messages.error)
      p.bg-danger
        = messages.error
block scripts
  script(src='/bundle.js')
